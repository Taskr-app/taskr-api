{
  "name": "server",
  "version": "0.0.1",
  "description": "Awesome project developed with TypeORM.",
  "private": true,
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.1",
    "@types/faker": "^4.1.6",
    "@types/graphql": "^14.5.0",
    "@types/hashids": "^1.0.30",
    "@types/hat": "^0.0.1",
    "@types/ioredis": "^4.0.18",
    "@types/jest": "^24.0.18",
    "@types/jsonwebtoken": "^8.3.4",
    "@types/lodash": "^4.14.149",
    "@types/node": "^12.7.5",
    "@types/nodemailer": "^6.2.1",
    "@types/pg-connection-string": "^2.0.0",
    "@types/uuid": "^3.4.6",
    "@typescript-eslint/eslint-plugin": "^2.15.0",
    "@typescript-eslint/parser": "^2.15.0",
    "apollo-server-testing": "^2.9.6",
    "babel-core": "^6.26.3",
    "babel-jest": "^24.9.0",
    "babel-preset-env": "^1.7.0",
    "concurrently": "^5.0.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-security": "^1.4.0",
    "jest": "^24.9.0",
    "nodemailer-stub": "^1.2.1",
    "nodemon": "^1.19.4",
    "prettier": "^1.19.1",
    "ts-jest": "^24.1.0",
    "ts-node-dev": "^1.0.0-pre.44",
    "tslint": "^5.20.0",
    "typeorm-fixtures-cli": "^1.3.7",
    "typescript": "3.6.3"
  },
  "dependencies": {
    "apollo-server-express": "^2.9.3",
    "axios": "^0.19.0",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.16.0",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "faker": "^4.1.0",
    "google-auth-library": "^5.4.1",
    "googleapis": "^43.0.0",
    "graphql": "^14.5.7",
    "graphql-redis-subscriptions": "^2.1.1",
    "hashids": "^2.0.1",
    "ioredis": "^4.14.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "moment": "^2.29.4",
    "nodemailer": "^6.3.1",
    "pg": "^7.12.1",
    "pg-connection-string": "^2.1.0",
    "reflect-metadata": "^0.1.10",
    "ts-node": "8.4.1",
    "type-graphql": "^0.17.5",
    "typeorm": "^0.2.19",
    "uuid": "^3.3.3"
  },
  "scripts": {
    "dev": "concurrently \"yarn db:start\" \"nodemon --exec ts-node src/index.ts\"",
    "dev:api": "ts-node-dev --respawn --transpileOnly --ignore-watch node_modules --no-notify src/index.ts",
    "start": "node dist/index.js",
    "build": "tsc --sourceMap false",
    "test": "jest --forceExit --runInBand",
    "db:start": "docker-compose up --build",
    "db:seed": "fixtures ./src/database/fixtures --config ./ormconfig.js --sync --require=ts-node/register",
    "cloudinary:flushall": "./src/services/cloudinary/del_resources.sh"
  },
  "jest": {
    "transform": {
      "^.+\\.ts?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(js?|ts?)$",
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/node_modules",
      "/src/__tests__/mocks/*"
    ],
    "moduleFileExtensions": [
      "ts",
      "js",
      "json",
      "node"
    ],
    "globals": {
      "ts-jest": {
        "babelConfig": true,
        "tsConfig": "tsconfig.json"
      }
    },
    "globalSetup": "<rootDir>/src/__tests__/mocks/setup.ts",
    "globalTeardown": "<rootDir>/src/__tests__/mocks/teardown.ts"
  }
}
